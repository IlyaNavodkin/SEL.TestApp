@model System.Collections.Generic.IEnumerable<SEL.DAL.Entities.Department>

@{
    ViewBag.Title = "Отделы";
    Layout = "_Layout";
}

<h2>Список отделов</h2>

<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Имя</th>
        <th>Id родительского отдела</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var worker in Model)
    {
        <tr>
            <td>@worker.Id</td>
            <td>@worker.Name</td>
            <td>@worker.ParentDepartmentId</td>
        </tr>
    }
    </tbody>
</table>